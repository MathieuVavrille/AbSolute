# Domains

param b := 1.0e+1;

var x1 >= -b, <= b;
var y1 >= -b, <= b;
var z1 >= -b, <= b;
var x2 >= -b, <= b;
var y2 >= -b, <= b;
var z2 >= -b, <= b;
var x3 >= -b, <= b;
var y3 >= -b, <= b;
var z3 >= -b, <= b;

subject to

e1: (x1^2) + (y1^2) + (z1^2) + (-31) = 0;

e2: (x2^2) + (y2^2) + (z2^2) + (-39) = 0;

e3: (x3^2) + (y3^2) + (z3^2) + (-29) = 0;

e4: x1*x2 + y1*y2 + z1*z2 + 6*x1 - 6*x2 + (-51) = 0;

e5: x1*x3 + y1*y3 + z1*z3 + 7*x1 - 2*y1 - 7*x3 + 2*y3 + (-50) = 0;

e6: x2*x3 + y2*y3 + z2*z3 + x2 - 2*y2 - x3 + 2*y3 + (-34) = 0;

e7: -12*x1 + 15*y1 - 10*x2 - 25*y2 + 18*x3 + 18*y3 + 32 = 0;

e8: -14*x1 + 35*y1 - 36*x2 - 45*y2 + 30*x3 + 18*y3 + (-8) = 0;

e9: 2*x1 + 2*y1 - 14*x2 - 2*y2 + 8*x3 - y3 + (-20) = 0;

solve;
print x1,x2,x3;
